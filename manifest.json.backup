{
  "manifest_version": 3,
  "name": "RainMark - 智能书签管理器",
  "version": "1.0.0",
  "description": "一个美观实用的智能书签管理扩展，具有自动分类和清理功能",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "history",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "RainMark",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
